<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.nyu.cf.user.User" table="users">
        <id name="uid" column="uid">
            <generator class="native"/>
        </id>
        <property name="username" column="uname"/>
        <property name="email" column="uemail"/>
        <property name="password" column="upassword"/>
        <property name="hometown" column="uhometown"/>
        <property name="interest" column="uinterest"/>
        <property name="credit_card" column="ucredit_card"/>
    </class>
</hibernate-mapping>